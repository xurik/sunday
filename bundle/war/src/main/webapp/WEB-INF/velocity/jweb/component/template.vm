    <form id="template_form" name="component_template" method="post" action="">
        <input id="componentCode" name="componentCode" value="$!componentCode" type="hidden"/>
        <label>解析器</label>
        <select name="resolver">
            <option value="velocity">Velocity</option>
        </select>
        <br />
        <textarea id="component_template" name="template">$!template</textarea>
        <br />

        <input id="saveTemplate" type="button" value="保存" class="j-button">
    </form>

